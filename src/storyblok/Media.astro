---
import { Image } from "astro:assets";

const { blok, index } = Astro.props;
const filename = blok.asset.filename;
---

{
  filename && (
    <Image
      id={`gallery-image-${index}`}
      class="h-full w-full object-contain"
      src={`${filename}/m/1920x0`}
      inferSize
      alt="image"
    />
  )
}
